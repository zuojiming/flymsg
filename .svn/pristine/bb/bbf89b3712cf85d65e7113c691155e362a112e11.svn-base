        <script type="text/javascript">
$(function() {
    $(".js-scroll").each(function() {
        // initialize scrollable
        $(".scroll-con").scrollable({
            circular: true,
            next: '.btn-scroll-prev',
            prev: '.btn-scroll-next',
            items: '.comm-glist-box'
        });
    });

    $(".js-rank").each(function() {
        $(this).find("li").each(function(i) {
            var j = i + 1;
            $(this).find("em").html(j);
        })
        $(this).find("li:lt(1)").addClass('top');
        $(this).find("li:lt(3)").addClass('top1');
    });

    $(".mod-gamelist .game-list").each(function(i) {
        if (i < 3)
        {
            $(this).addClass("game-list-first")
        }
    })

    //js-tab
    $(".js-tab").each(function() {
        var time = 100, t;
        $.tools.tabs.addEffect("myEffect", function(tabIndex, done) {
            clearTimeout(t);
            var obj = this.getPanes();

            t = setTimeout(function() {
                obj.hide().eq(tabIndex).show();
            }, time);
            done.call();

        });
        $(".gb-tab:eq(0)", $(this)).tabs($(".gb-tab-pn:eq(0) > div", $(this)), {event: 'mouseover', effect: 'myEffect'});
    });

    //js-click-tab
    $(".js-click-tab").each(function() {
        $(".gb-tab:eq(0)", $(this)).tabs($(".gb-tab-pn:eq(0) > div", $(this)), {event: 'mouseover', onClick: function(e, i) {
                if (i > 0) {
                    $(".mod-bd-category .comm-glist-box-w").eq(i).find("img[data-src]").each(function(index, element) {
                        $(this).attr("src", $(this).attr("data-src"));
                        $(this).removeAttr("data-src");
                    });                }
            }
        });
    });

    //focus-con
    $(".focus-con").each(function() {
        $(".focus-con-tab").tabs(".focus-con-img .item", {
            event: 'mouseover',
            rotate: true
        }).slideshow({autoplay: true, interval: 4000});
    });


    var x, y, tipcnt;

    $('.recom-list li').each(function(j) {
        var $this = $(this);
        $this.mousemove(function(event) {
            x = event.pageX + 15;
            y = event.pageY - 70;
            $(this).find(".tool-tip").css({left: x, top: y, position: 'absolute', display: 'block', zIndex: 544});

        }).mouseout(function() {
            $(".tool-tip").hide();

        });

    });

    if ($(".side-info-box").length > 0)
    {
        $(".close-btn").click(function() {
            $(".side-info-box").hide();
        })
    }
    if ($(".fix-box").length > 0)
    {

        var $backToTopEle = $(".fix-box"),
                $backToTopFun = function() {
                    var st = $(document).scrollTop(),
                            winh = $(window).height();
                    //IE6
                    if (!window.XMLHttpRequest) {
                        $backToTopEle.css("top", st + 350);
                    }
                };
        $(window).bind("scroll", $backToTopFun);
    }


    $(".search-box .s-txt").focus(function() {
        $(".search-box").addClass("search-box-on")
    }).blur(function() {
        $(".search-box").removeClass("search-box-on");
    });


    $(window).scroll(function() {
        if ($(window).scrollTop() > 0) {
            $('.bttop').css("display", "block");
        } else {
            $('.bttop').hide();
        }
    });

    $("#addBookMark").click(function() {
        if (window.ActiveXObject) {
            var n = window.location.href,
                    t = window.title;
            window.external.addFavorite(n, t)
        } else
            alert("\u8bf7\u6309Ctrl+D\u5c06\u672c\u9875\u6dfb\u52a0\u5230\u6536\u85cf\u5939")
    });

    if ($(".lazyloadbox").length > 0) {
        $(".lazyloadbox").imgLazyLoad();
    }

    //played-game
	(function(){
		function getCookie(c_name){
			if (document.cookie.length>0){
				c_start=document.cookie.indexOf(c_name + "=");
				if (c_start!=-1){
					c_start=c_start + c_name.length+1;
					c_end=document.cookie.indexOf(";",c_start);
					if (c_end==-1) c_end=document.cookie.length;
					return unescape(document.cookie.substring(c_start,c_end));
				}
			}
			return "";
		}

		function setCookie(c_name,value,expiredays){
			var exdate=new Date();
			exdate.setDate(exdate.getDate()+expiredays);
			document.cookie=c_name+ "=" +escape(value)+
			((expiredays==null) ? "" : ";expires="+exdate.toGMTString());
		}
		var playHtml = $('.played-game .mod-bd ul').html();
		function g_init(){
			var g = getCookie('_____GLIST2'),glist = [],plen = $('.played-game .comm-glist').length;
			if(g){
				$.each(g.split('|'),function(i,v){
					if(v=='')return;
					var j = '<li class="comm-glist"><a href="'+v.split(',')[0]+'" target="_blank" title="" class="item"><span class="img"><img src="'+v.split(',')[1]+'" width="52" height="52" alt=""/></span><span class="txt txt-ex">'+v.split(',')[2]+'</span></a></li>';
					glist[i] = j;

				});

				$('.played-game .mod-bd ul').html(playHtml).prepend(glist.join(''));

			}
			if( (plen > 9)||(g.split('|').length + plen > 9)){
				$('.vscroll-btn').show();
			}


		}
		g_init();

		$('.comm-glist a,.comm-glist1 a').click(function(){
			var txt = $(this).find('.txt').text();
			if($(this).find('.txt span').hasClass('tit')){
				txt = $(this).find('.tit').text();
			}
			var st = $(this).attr('href')+','+$(this).find('img').attr('src')+','+txt;
			var gc = getCookie('_____GLIST2')?getCookie('_____GLIST2'):'';
			if(gc.split('|').length >15){
				var c = gc.split('|');
				c.pop();
				gc = c.join('|');
			}
			var c1 = c2 = gc.split('|');
			for(var ii = 0;ii <gc.split('|').length;ii++){
				if(st == gc.split('|')[ii]){
					if(ii == 0){
						c1.shift();
						gc = c1.join('|');
					}
					else if((ii+1) == gc.split('|').length){
						c1.pop();
						gc = c1.join('|');
					}
					else
					{
						gc = c1.slice(0,ii).concat(c2.slice(ii+1)).join('|');
						console.log(ii,gc)
					}
				}
			}
			setCookie('_____GLIST','');
			setCookie('_____GLIST2',st+'|'+gc,7);
			g_init();

		})
		var v = 0;
		$('.vscroll-btn a').click(function(){
			if($('.played-game .comm-glist-box').is(':animated')){return false;}
			var m = 0-Math.ceil($('.played-game .mod-bd ul').height()/172)+1;
			if((v == 0 && $(this).data('v')=='1')||(v==m && $(this).data('v')=='-1')){return false;}
			v = v + (+$(this).data('v'));

			var margin_top = +$('.played-game .comm-glist-box').css('marginTop').split('px')[0];
			$('.played-game .comm-glist-box').animate({'marginTop':margin_top+172*(+$(this).data('v'))+'px'});
			return false;
		})

	})();


});

        </script>
