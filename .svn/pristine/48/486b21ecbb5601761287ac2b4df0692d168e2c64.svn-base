/*mod-game-cla*/
.mod-game-cla{border-top:2px solid #1690cc;}
.mod-game-cla .mod-hd{padding:0;background:#edf4f8;border-bottom:1px solid #d2e1f1;height:44px;line-height:44px;}
.mod-game-cla .mod-hd .item,.mod-game-cla .mod-hd .sline{float:left;}
