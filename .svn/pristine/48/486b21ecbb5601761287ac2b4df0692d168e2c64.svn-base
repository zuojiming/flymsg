.nav-list .item .link{display:inline-block;padding:1px 6px;line-height:1.3em;color:#fff;}
.nav-list .item .link:hover{padding:0 5px;background-color:#fff;border:1px solid #0072b2;color:#2195d6;-moz-border-radius: 4px;-webkit-border-radius: 4px;border-radius:4px;}

.header-tool{padding:6px 10px;background-color:#f6f6f6;border:1px solid #e1e1e1;}
.search-box{width:380px;height:34px;border:1px solid #d9d9d9;background-color:#fff;position:relative;overflow:hidden;}
.search-box-on{border-color:#9d9d9d;}
.search-box input{background:none;border:none;}
.search-box .search-txt{}
.search-box .s-txt{padding:8px 60px 8px 10px;width:310px;line-height:18px;height:18px;color:#999;}
.search-box-on .s-txt{color:#000;}
.search-box .search-btn{position:absolute;right:0;top:0;width:60px;height:34px;border-left:1px solid #eaeaea;}
.search-box .search-btn .ico-search{position:absolute;top:6px;left:35%;}
.search-box .search-btn:hover{background-color:#ff4800;}
.search-box .s-btn{width:100%;height:100%;position:absolute;top:0;left:0; cursor:pointer;}
.search-box .search-btn:hover .ico-search{background-position:-130px -128px;}

.hot-link{line-height:34px;margin-left:10px;}
.hot-link .hd{color:#fd0000;font-weight:bold;}
.hot-link .item{color:#5b5b5b;margin-right:10px;}
.hot-link .item:hover{ text-decoration:underline;}

.top-btn .btn{line-height:32px;text-align:center;color:#fff;padding-left:18px;width:90px;}


/*content*/
.content{width:1000px;margin:0 auto;}

.mod-gamelist{background-color:#fff;border:1px solid #c7e2f2;border-top:none;padding:10px;overflow:hidden;*zoom:1;}
