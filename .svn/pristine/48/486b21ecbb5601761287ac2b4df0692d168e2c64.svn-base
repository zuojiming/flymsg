body{font:normal 12px/22px "SimSun";min-width:1000px;background-color:#f6f6f6;}
.wrap{}
.wrap-in{background:#f9f1cd url(../images/bg.jpg) no-repeat center top;padding-bottom:20px; padding-top:200px;}
a{ cursor:pointer;}
a:hover{ direction:ltr;}

/*commom css*/
.ico,.btn{display:inline-block;background:url(http://ue2.17173cdn.com/a/flash/index/2014/img/sprites.png) no-repeat; vertical-align:middle;overflow:hidden;}
.ico-tit{width:10px;height:18px;background-position:-120px -110px;margin-right:5px;}
.ico-tit1{width:5px;height:35px;background-position:0 -560px;}
.ico-tit1-ex{background-position:-20px -560px;}
.ico-tit1-ex1{background-position:-40px -560px;}
.ico-search{width:22px;height:22px;background-position:0 0;}
.ico-dot{width:4px;height:6px;background-position:right -110px;}
.ico-btop{width:10px;height:8px;background-position:-109px -110px;_background-position:-108px -110px;}
.ico-point{width:12px;height:6px;background-position:-130px -110px;}
.ico-arrow{width:12px;height:26px;background-position:right -128px;}
.ico-tj{width:14px;height:16px;margin-right:3px;*margin-top:3px;background:url(http://ue2.17173cdn.com/a/flash/index/2014/img/ico-tj.jpg) no-repeat;}

.p-ico{display:inline-block;background:url(http://ue1.17173cdn.com/a/flash/index/2014/img/p-ico.png) no-repeat; position:absolute;}
.p-ico-mx{width:50px;height:50px;background-position:0 0;}
.p-ico-sj{width:75px;height:42px;background-position:0 -70px;}
.p-ico-dz{width:42px;height:48px;background-position:0 -130px;}
.p-ico-xx{width:56px;height:32px;background-position:0 -200px;}
.p-ico-cl{width:48px;height:32px;background-position:0 -240px;}
.p-ico-yz{width:42px;height:37px;background-position:0 -290px;}
.p-ico-mj{width:51px;height:45px;background-position:0 -340px;}
.p-ico-ty{width:46px;height:52px;background-position:0 -400px;}
.p-ico-sr{width:52px;height:40px;background-position:0 -470px;}
.p-ico-zb{width:38px;height:48px;background-position:0 -520px;}
.p-ico-et{width:58px;height:50px;background-position:0 -590px;}
.p-ico-zw{width:32px;height:48px;background-position:0 -660px;}
.p-ico-qb{width:40px;height:40px;background-position:0 -720px;}

.btn-classify{width:40px;height:18px;line-height:18px;_line-height:20px;padding-right:6px;text-align:center;background-position:-35px 0;color:#666;}
.btn-upload{width:108px;height:32px;background-position:0 -30px;}
.btn-upload:hover{background-position:-110px -30px;}
.btn-safe{width:108px;height:32px;background-position:0 -70px;}
.btn-safe:hover{background-position:-110px -70px;}
.btn-scroll-prev{width:18px;height:70px;background-position:0 -110px;}
.btn-scroll-next{width:18px;height:70px;background-position:-35px -110px;}

.js-vscroll-btn .mod-hd{position:relative;}
.vscroll-btn a{display:block;width:15px;height:10px;background:url(http://ue1.17173cdn.com/a/flash/index/2014/img/btn.png) no-repeat;position:absolute;top:20px;}
.vscroll-btn .vscroll-prev{right:35px;}
.vscroll-btn .vscroll-next{background-position:0 -10px;right:15px;}

.arrow{width:0;height:0;line-height:0;border-width:4px;border-style:solid; border-color:  transparent transparent #2397d8 transparent; border-style: dashed dashed solid dashed;position:absolute;bottom:8px;right:5px;}
/*comm-glist*/
.comm-glist{float:left;padding:5px 4px;}
.comm-glist .item{display:block;width:64px; text-align:center;color:#262626;white-space:nowrap;overflow:hidden;}
.comm-glist .item:hover{text-decoration:underline; color:#0086d1}
.comm-glist .item span{display:block;}
.comm-glist .item .img{padding-bottom:2px;}
.comm-glist .item img{width:52px;height:52px;-moz-border-radius:10px;-webkit-border-radius: 10px;border-radius:10px;}
.comm-glist-box{overflow:hidden;*zoom:1;}
.comm-glist-ex{padding:6px 4px ;}
.comm-glist-ex .item{width:90px;}
.comm-glist-ex .item img{width:75px;height:75px;}
.comm-glist-ex .item .txt{margin-top:5px;}
/*comm-glist1*/
.comm-glist1 .item{display:block;overflow:hidden;*zoom:1;color:#000;}
.comm-glist1 .item:hover .txt .tit{text-decoration:underline;}
.comm-glist1 .item em{float:left;width:20px;font-size:14px;font-weight:bold;color:#999;margin:15px 3px 0 0; text-align:center;}
.comm-glist1 .item .img{float:left;padding-bottom:2px;}
.comm-glist1 .item .img img{width:52px;height:52px;-moz-border-radius:10px;-webkit-border-radius: 10px;border-radius:10px;}
.comm-glist1 .item .txt{display:block;padding:5px 0 0 80px;}
.comm-glist1 .item .txt .tit{display:block;color:#000;overflow:hidden; text-overflow:ellipsis; white-space:nowrap; _width:83px;}
.comm-glist1 .item:hover .tit{ color:#0086d1}
.comm-glist1 .item .txt .count{display:block;color:#595959;overflow:hidden; text-overflow:ellipsis; white-space:nowrap; _width:83px;}
.comm-glist1 .item .txt .count .num{color:#ff4800;}
.comm-rank-box .top1 em{color:#2094d5}
.comm-rank-box .top em{color:#ff4800}
.comm-rank-box li{padding:4px 0;}

/*comm-rank*/
.comm-rank .rank-hd{margin-top:-1px;border:1px solid #f6f6f6;background-color:#f6f6f6;border-top:1px solid #e1e1e1;border-bottom:1px solid #e1e1e1;overflow:hidden;*zoom:1;}
.comm-rank .rank-hd .item{width:33.3%;float:left;background-color:#fff;margin-left:-1px;text-align:center;border-left:1px solid #e1e1e1;color:#333;line-height:3em;}
.comm-rank .rank-hd .current{background-color:#f6f6f6;}
.comm-rank .rank-bd{padding:5px 8px;background-color:#efefef;}
.comm-rank .js-rank{display:none;}
.comm-rank-ex{margin-top:20px;}
.comm-rank-ex .rank-hd .item{width:49.9%;}

/*comm-plist*/
.comm-plist {text-align:center;padding:10px 0;}
.comm-plist .item{display:inline-block;width:154px;color:#000;}
.comm-plist .item:hover{text-decoration:underline; color: #5893d0}
.comm-plist .item img{width:154px;height:86px;}
.comm-plist .item span{display:block;}
.comm-plist .item .txt{line-height:2.4em;}

.comm-plist-box{overflow:hidden;*zoom:1;}
.comm-plist-ex{float:left;padding:10px 0 7px 10px}
.comm-plist-ex .item{width:120px;}
.comm-plist-ex .item img{width:110px;height:62px;margin-top:4px;}
.comm-plist-ex .item .img{width:116px;height:74px;background:url(http://ue2.17173cdn.com/a/flash/index/2014/img/sprites.png) no-repeat 0 -180px;}
/*begin*/

.header{width:1000px;margin:0 auto;}
.header-c1{padding:15px 0;overflow:hidden;*zoom:1;}
.header-c1 .logo{padding:5px 0 0 50px;float:left;}
.header-c1 .top-ad{float:right;}
/*nav*/
.nav{overflow:hidden;*zoom:1;}
.main-nav{float:left;}
.side-nav{float:right;}
.nav-list {overflow:hidden;*zoom:1;font-size:16px;font-family:"Microsoft YaHei";background-color:#0072b2;}
.nav-list .item{padding:7px 1px;float:left;}
.nav-list .item-ex{padding:7px 1px;background-color:#0086d1;}
.nav-list .item-first{padding-left:4px;}
