.mod-game-cla .gb-tab .current .p-ico-sj,.mod-game-cla .gb-tab .current .p-ico-xx{left:0;} 
.mod-game-cla .gb-tab .current .p-item{display:block;height:49px;width:50px;_width:60px;padding-left:40px;_padding-left:30px;position:absolute;left:-10px;background:#fff url(http://ue2.17173cdn.com/a/flash/index/2014/img/sprites.png) repeat-x 0 -500px;border:1px solid #51b3e3;border-bottom:none;border-top:4px solid #41a6d7;-moz-border-radius: 2px 2px 0 0;-webkit-border-radius:  2px 2px 0 0;border-radius: 2px 2px 0 0;}
