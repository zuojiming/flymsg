                $(this).find("li").each(function (i) {
                    var j = i + 1;
                    $(this).find("em").html(j);
                })
                $(this).find("li:lt(1)").addClass('top');
                $(this).find("li:lt(3)").addClass('top1');
            });

            $(".mod-gamelist .game-list").each(function (i) {
                if (i < 3) {
                    $(this).addClass("game-list-first")
                }
            })

            //js-tab
            $(".js-tab").each(function () {
                var time = 100, t;
                $.tools.tabs.addEffect("myEffect", function (tabIndex, done) {
                    clearTimeout(t);
                    var obj = this.getPanes();

                    t = setTimeout(function () {
                        obj.hide().eq(tabIndex).show();
                    }, time);
                    done.call();

                });
                $(".gb-tab:eq(0)", $(this)).tabs($(".gb-tab-pn:eq(0) > div", $(this)), { event: 'mouseover', effect: 'myEffect' });
            });

            //js-click-tab
            $(".js-click-tab").each(function () {
                $(".gb-tab:eq(0)", $(this)).tabs($(".gb-tab-pn:eq(0) > div", $(this)), {
                    event: 'mouseover', onClick: function (e, i) {
                        if (i > 0) {
                            $(".mod-bd-category .comm-glist-box-w").eq(i).find("img[data-src]").each(function (index, element) {
                                $(this).attr("src", $(this).attr("data-src"));
                                $(this).removeAttr("data-src");
                            });
                        }
                    }
                });
            });

            //focus-con
            $(".focus-con").each(function () {
                $(".focus-con-tab").tabs(".focus-con-img .item", {
                    event: 'mouseover',
                    rotate: true
                }).slideshow({ autoplay: true, interval: 4000 });
            });


            var x, y, tipcnt;

            $('.recom-list li').each(function (j) {
                var $this = $(this);
                $this.mousemove(function (event) {
                    x = event.pageX + 15;
                    y = event.pageY - 70;
                    $(this).find(".tool-tip").css({ left: x, top: y, position: 'absolute', display: 'block', zIndex: 544 });

                }).mouseout(function () {
                    $(".tool-tip").hide();

                });

            });

            if ($(".side-info-box").length > 0) {
                $(".close-btn").click(function () {
                    $(".side-info-box").hide();
                })
            }
            if ($(".fix-box").length > 0) {

                var $backToTopEle = $(".fix-box"),
                        $backToTopFun = function () {
                            var st = $(document).scrollTop(),
                                    winh = $(window).height();
                            //IE6
                            if (!window.XMLHttpRequest) {
                                $backToTopEle.css("top", st + 350);
                            }
                        };
                $(window).bind("scroll", $backToTopFun);
            }


