                    window.external.addFavorite(n, t)
                } else
                    alert("\u8bf7\u6309Ctrl+D\u5c06\u672c\u9875\u6dfb\u52a0\u5230\u6536\u85cf\u5939")
            });

            if ($(".lazyloadbox").length > 0) {
                $(".lazyloadbox").imgLazyLoad();
            }

            //played-game
            (function () {
                function getCookie(c_name) {
                    if (document.cookie.length > 0) {
                        c_start = document.cookie.indexOf(c_name + "=");
                        if (c_start != -1) {
                            c_start = c_start + c_name.length + 1;
                            c_end = document.cookie.indexOf(";", c_start);
                            if (c_end == -1) c_end = document.cookie.length;
                            return unescape(document.cookie.substring(c_start, c_end));
                        }
                    }
                    return "";
                }

                function setCookie(c_name, value, expiredays) {
                    var exdate = new Date();
                    exdate.setDate(exdate.getDate() + expiredays);
                    document.cookie = c_name + "=" + escape(value) +
                    ((expiredays == null) ? "" : ";expires=" + exdate.toGMTString());
                }
                var playHtml = $('.played-game .mod-bd ul').html();
                function g_init() {
                    var g = getCookie('_____GLIST2'), glist = [], plen = $('.played-game .comm-glist').length;
                    if (g) {
                        $.each(g.split('|'), function (i, v) {
                            if (v == '') return;
                            var j = '<li class="comm-glist"><a href="' + v.split(',')[0] + '" target="_blank" title="" class="item"><span class="img"><img src="' + v.split(',')[1] + '" width="52" height="52" alt=""/></span><span class="txt txt-ex">' + v.split(',')[2] + '</span></a></li>';
                            glist[i] = j;

                        });

                        $('.played-game .mod-bd ul').html(playHtml).prepend(glist.join(''));

                    }
                    if ((plen > 9) || (g.split('|').length + plen > 9)) {
                        $('.vscroll-btn').show();
                    }


                }
                g_init();

                $('.comm-glist a,.comm-glist1 a').click(function () {
                    var txt = $(this).find('.txt').text();
                    if ($(this).find('.txt span').hasClass('tit')) {
                        txt = $(this).find('.tit').text();
                    }
                    var st = $(this).attr('href') + ',' + $(this).find('img').attr('src') + ',' + txt;
                    var gc = getCookie('_____GLIST2') ? getCookie('_____GLIST2') : '';
                    if (gc.split('|').length > 15) {
                        var c = gc.split('|');
                        c.pop();
                        gc = c.join('|');
                    }
                    var c1 = c2 = gc.split('|');
                    for (var ii = 0; ii < gc.split('|').length; ii++) {
                        if (st == gc.split('|')[ii]) {
                            if (ii == 0) {
                                c1.shift();
                                gc = c1.join('|');
                            }
                            else if ((ii + 1) == gc.split('|').length) {
                                c1.pop();
                                gc = c1.join('|');
                            }
                            else {
                                gc = c1.slice(0, ii).concat(c2.slice(ii + 1)).join('|');
                                console.log(ii, gc)
                            }
                        }
                    }
                    setCookie('_____GLIST', '');
                    setCookie('_____GLIST2', st + '|' + gc, 7);
                    g_init();

                })
                var v = 0;
                $('.vscroll-btn a').click(function () {
                    if ($('.played-game .comm-glist-box').is(':animated')) { return false; }
                    var m = 0 - Math.ceil($('.played-game .mod-bd ul').height() / 172) + 1;
                    if ((v == 0 && $(this).data('v') == '1') || (v == m && $(this).data('v') == '-1')) { return false; }
                    v = v + (+$(this).data('v'));

                    var margin_top = +$('.played-game .comm-glist-box').css('marginTop').split('px')[0];
                    $('.played-game .comm-glist-box').animate({ 'marginTop': margin_top + 172 * (+$(this).data('v')) + 'px' });
                    return false;
                })

            })();


        });

    </script>
